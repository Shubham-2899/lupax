{
  "name": "lupax.",
  "description": "lupax extension",
  "author": "www.lupax.app",
  "version": "1.4.4",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "scripting",
    "cookies",
    "storage"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://lupax.app/*",
      "*://www.lupax.app/*",
      "*://develop.lupax.app/*"
    ]
  },
  "icons": {
    "16": "icons/lupax-icon-16.png",
    "32": "icons/lupax-icon-32.png",
    "48": "icons/lupax-icon-48.png",
    "128": "icons/lupax-icon-128.png"
  },
  "host_permissions": [
    "*://*/*"
  ]
}